{
  "name": "@graphql-inspector/github",
  "version": "0.0.0-PLACEHOLDER",
  "description": "GraphQL Inspector's Probot. Compare GraphQL Schemas, check documents, find breaking changes, find similar types.",
  "keywords": [
    "graphql",
    "graphql-inspector",
    "tools",
    "probot",
    "probot-plugin",
    "github"
  ],
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector",
    "directory": "packages/github"
  },
  "scripts": {
    "start": "probot run ./dist/index.js",
    "dev": "yarn build && yarn start",
    "test": "jest",
    "build": "tsc",
    "clean": "rm -rf dist/",
    "deploy": "now --docker",
    "prebuild": "yarn clean",
    "prepublishOnly": "yarn build",
    "prepack": "bob prepack"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0",
    "probot": "^9.0.1 || ^10.0.0 || ^11.0.0"
  },
  "dependencies": {
    "@graphql-inspector/core": "0.0.0-PLACEHOLDER",
    "axios": "0.21.1",
    "dataloader": "2.0.0",
    "js-yaml": "4.1.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "probot": "11.3.0",
    "smee-client": "1.2.2"
  }
}
